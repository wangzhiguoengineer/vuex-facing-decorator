{
  "name": "vuex-facing-decorator",
  "version": "1.0.0",
  "description": "Vuex decorator specifically designed for Vue3.",
  "type": "commonjs",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "typings": "dist/typings/index.d.js",
  "scripts": {
    "build": "npm run clean && npm run build:esm && npm run build:cjs && npm run build:umd && npm run build:typings",
    "build:esm": "tsc -outDir dist/esm -module ES6",
    "build:cjs": "tsc -outDir dist/cjs -module commonjs",
    "build:umd": "tsc -outDir dist/umd -module umd",
    "build:typings": "tsc -declaration true -emitDeclarationOnly true -declarationDir dist/typings",
    "clean": "rimraf dist",
    "test": "mocha"
  },
  "author": "wangzhiguo.engineer@gmail.com",
  "license": "MIT",
  "peerDependencies": {
    "vue": "^3.0.0",
    "vue-facing-decorator": "^3.0.0",
    "vuex": "^4.0.0"
  },
  "devDependencies": {
    "@types/chai": "^4.3.5",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.4.5",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "@vue/test-utils": "^2.4.1",
    "chai": "^4.3.7",
    "eslint": "^8.46.0",
    "jsdom": "^22.1.0",
    "jsdom-global": "^3.0.2",
    "mocha": "^10.2.0",
    "rimraf": "^5.0.1",
    "sinon": "^15.2.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/wangzhiguoengineer/vuex-facing-decorator",
  "repository": {
    "type": "git",
    "url": "git@github.com:wangzhiguoengineer/vuex-facing-decorator.git"
  },
  "publishConfig": {
    "registry": "https://github.com/wangzhiguoengineer/vuex-facing-decorator.git"
  }
}
